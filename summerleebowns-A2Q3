
function setup() {
  createCanvas(600, 400);}



function drawStar (starX,starY,StarDiameter) 
{ beginShape();
vertex(360, 290);
vertex(340, 220);
vertex(400, 175);
vertex(330, 175);
vertex(300, 100);
vertex(270,175);
vertex(200, 175);
vertex(260, 220);
vertex(240, 290);
vertex(300, 250);
vertex(360, 290);
 endShape();
}
  
 function draw() {

  background(10,150,225);
  drawStar();
}

